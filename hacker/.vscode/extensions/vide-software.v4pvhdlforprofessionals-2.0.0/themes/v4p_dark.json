{
  "$schema": "vscode://schemas/color-theme",
  "name": "V4P - VHDL (Visual Studio dark)",
  "include": "./dark_plus.json",
  "semanticHighlighting": true,
  "type": "dark",
  "colors": {
	"editor.background": "#0F0F0F",
	},
	"tokenColors": [
		{"scope": ["source.v4p.vhdl"],                              "settings": {"foreground": "#aaacbb", "fontStyle": ""}},
		{"scope": ["string.quoted.double.v4p.vhdl"],                "settings": {"foreground": "#bb7e62", "fontStyle": ""}},
		{"scope": ["punctuation.v4p.vhdl"],                         "settings": {"foreground": "#FFFFFFFF", "fontStyle": ""}},
		{"scope": ["keyword.control.v4p.vhdl"],                     "settings": {"foreground": "#5f6fc9", "fontStyle": "bold"}},
		{"scope": ["keyword.operator.word.v4p.vhdl"],               "settings": {"foreground": "#5e70da", "fontStyle": "bold"}},
		{"scope": ["keyword.operator.v4p.vhdl"],                    "settings": {"foreground": "#FFFFFFFF", "fontStyle": ""}},
		{"scope": ["comment.line.preprocessor.v4p.vhdl"],           "settings": {"foreground": "#A8A8A8FF", "fontStyle": ""}},
		{"scope": ["comment.line.conditional.v4p.vhdl"],            "settings": {"foreground": "#A8A8A8FF", "fontStyle": ""}},
		{"scope": ["comment.line.conditionalinactive.v4p.vhdl"],    "settings": {"foreground": "#585858FF", "fontStyle": ""}},
		{"scope": ["comment.line.protect.v4p.vhdl"],                "settings": {"foreground": "#A8A8A8FF", "fontStyle": ""}},
		{"scope": ["comment.line.v4p.vhdl"],                        "settings": {"foreground": "#70995e", "fontStyle": ""}},    
		{"scope": ["comment.block.v4p.vhdl"],                       "settings": {"foreground": "#70995e", "fontStyle": ""}},    
		{"scope": ["entity.name.identifier.v4p.vhdl"],              "settings": {"foreground": "#aaacbb", "fontStyle": ""}},
		{"scope": ["entity.name.identifier.extended.v4p.vhdl"],     "settings": {"foreground": "#aaacbb", "fontStyle": "", "background": "#666666FF"}},
		{"scope": ["entity.name.function.v4p.vhdl"],                "settings": {"foreground": "#8a9dc5", "fontStyle": "bold"}},
		{"scope": ["entity.name.type.v4p.vhdl"],                    "settings": {"foreground": "#4ee9ca", "fontStyle": ""}},
		{"scope": ["entity.name.type.enum.v4p.vhdl"],               "settings": {"foreground": "#569CD6FF", "fontStyle": ""}},
		{"scope": ["entity.name.type.unit.v4p.vhdl"],               "settings": {"foreground": "#569CD6FF", "fontStyle": ""}},
		{"scope": ["entity.name.type.record.v4p.vhdl"],             "settings": {"foreground": "#4ee9ca", "fontStyle": ""}},
		{"scope": ["entity.name.type.protected.v4p.vhdl"],          "settings": {"foreground": "#4ee9ca", "fontStyle": ""}},
		{"scope": ["entity.name.section.label.v4p.vhdl"],           "settings": {"foreground": "#a6b6dba6", "fontStyle": ""}},
		{"scope": ["entity.name.section.v4p.vhdl"],                 "settings": {"foreground": "#a6b6dba6", "fontStyle": ""}},
		{"scope": ["entity.other.attribute-name.v4p.vhdl"],         "settings": {"foreground": "#af4141", "fontStyle": ""}},
		{"scope": ["constant.numeric.bit_string_literal.v4p.vhdl"], "settings": {"foreground": "#96c57d", "fontStyle": "", "background": "#666666FF"}},
		{"scope": ["constant.numeric.float.v4p.vhdl"],              "settings": {"foreground": "#96c57d", "fontStyle": ""}},
		{"scope": ["constant.numeric.based_literal.v4p.vhdl"],      "settings": {"foreground": "#96c57d", "fontStyle": ""}},
		{"scope": ["constant.numeric.integer.v4p.vhdl"],            "settings": {"foreground": "#96c57d", "fontStyle": ""}},
		{"scope": ["constant.character.v4p.vhdl"],                  "settings": {"foreground": "#D69D85FF", "fontStyle": ""}},
		{"scope": ["constant.other.generics.v4p.vhdl"],             "settings": {"foreground": "#569CD6FF", "fontStyle": ""}},
		{"scope": ["constant.other.v4p.vhdl"],                      "settings": {"foreground": "#569CD6FF", "fontStyle": ""}},
		{"scope": ["variable.other.signal.v4p.vhdl"],               "settings": {"foreground": "#a382cf", "fontStyle": ""}},
		{"scope": ["variable.other.v4p.vhdl"],                      "settings": {"foreground": "#945aff", "fontStyle": ""}},
		{"scope": ["variable.parameter.v4p.vhdl"],                  "settings": {"foreground": "#1298A7FF", "fontStyle": ""}},
		{"scope": ["variable.parameter.in.v4p.vhdl"],               "settings": {"foreground": "#1298A7FF", "fontStyle": ""}},
		{"scope": ["variable.parameter.out.v4p.vhdl"],              "settings": {"foreground": "#1298A7FF", "fontStyle": ""}},
		{"scope": ["variable.parameter.inout.v4p.vhdl"],            "settings": {"foreground": "#1298A7FF", "fontStyle": ""}},
		{"scope": ["entity.name.interface.v4p.vhdl"],               "settings": {"foreground": "#a6b6dba6", "fontStyle": ""}},
		{"scope": ["meta.namespace.v4p.vhdl"],                      "settings": {"foreground": "#a6b6dba6", "fontStyle": ""}}
	]
}