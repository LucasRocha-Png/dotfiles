{
  "$schema": "vscode://schemas/color-theme",
  "name": "V4P - VHDL (Visual Studio light)",
  "include": "./light_plus.json",
	"semanticHighlighting": true,
	"type": "light",
	"colors": {
	},
	"tokenColors": [
		{"scope": ["source.v4p.vhdl"],                              "settings": {"foreground": "#000000FF", "fontStyle": ""}},
		{"scope": ["string.quoted.double.v4p.vhdl"],                "settings": {"foreground": "#800000FF", "fontStyle": ""}},
		{"scope": ["punctuation.v4p.vhdl"],                         "settings": {"foreground": "#000000FF", "fontStyle": ""}},
		{"scope": ["keyword.control.v4p.vhdl"],                     "settings": {"foreground": "#0000FFFF", "fontStyle": "bold"}},
		{"scope": ["keyword.operator.word.v4p.vhdl"],               "settings": {"foreground": "#0000FFFF", "fontStyle": "bold"}},
		{"scope": ["keyword.operator.v4p.vhdl"],                    "settings": {"foreground": "#000000FF", "fontStyle": ""}},
		{"scope": ["comment.line.preprocessor.v4p.vhdl"],           "settings": {"foreground": "#808080FF", "fontStyle": ""}},
		{"scope": ["comment.line.conditional.v4p.vhdl"],            "settings": {"foreground": "#808080FF", "fontStyle": ""}},
		{"scope": ["comment.line.conditionalinactive.v4p.vhdl"],    "settings": {"foreground": "#a0a0a0FF", "fontStyle": ""}},
		{"scope": ["comment.line.protect.v4p.vhdl"],                "settings": {"foreground": "#808080FF", "fontStyle": ""}},
		{"scope": ["comment.line.v4p.vhdl"],                        "settings": {"foreground": "#007900FF", "fontStyle": ""}},    
		{"scope": ["comment.block.v4p.vhdl"],                       "settings": {"foreground": "#007900FF", "fontStyle": ""}},    
		{"scope": ["entity.name.identifier.v4p.vhdl"],              "settings": {"foreground": "#000000FF", "fontStyle": ""}},
		{"scope": ["entity.name.function.v4p.vhdl"],                "settings": {"foreground": "#000000FF", "fontStyle": "bold"}},
		{"scope": ["entity.name.type.v4p.vhdl"],                    "settings": {"foreground": "#2F4E80FF", "fontStyle": ""}},
		{"scope": ["entity.name.type.enum.v4p.vhdl"],               "settings": {"foreground": "#0000FFFF", "fontStyle": ""}},
		{"scope": ["entity.name.type.unit.v4p.vhdl"],               "settings": {"foreground": "#0000FFFF", "fontStyle": ""}},
		{"scope": ["entity.name.type.record.v4p.vhdl"],             "settings": {"foreground": "#2F4E80FF", "fontStyle": ""}},
		{"scope": ["entity.name.type.protected.v4p.vhdl"],          "settings": {"foreground": "#2F4E80FF", "fontStyle": ""}},
		{"scope": ["entity.name.section.label.v4p.vhdl"],           "settings": {"foreground": "#919191FF", "fontStyle": ""}},
		{"scope": ["entity.name.section.v4p.vhdl"],                 "settings": {"foreground": "#816641FF", "fontStyle": ""}},
		{"scope": ["entity.name.identifier.extended.v4p.vhdl"],     "settings": {"foreground": "#000000FF", "fontStyle": "", "background": "#E1E1E1FF"}},
		{"scope": ["entity.other.attribute-name.v4p.vhdl"],         "settings": {"foreground": "#D50000FF", "fontStyle": ""}},
		{"scope": ["constant.numeric.bit_string_literal.v4p.vhdl"], "settings": {"foreground": "#FF00FFFF", "fontStyle": "", "background": "#FFDFEFFF"}},
		{"scope": ["constant.numeric.float.v4p.vhdl"],              "settings": {"foreground": "#FF00FFFF", "fontStyle": ""}},
		{"scope": ["constant.numeric.based_literal.v4p.vhdl"],      "settings": {"foreground": "#FF00FFFF", "fontStyle": ""}},
		{"scope": ["constant.numeric.integer.v4p.vhdl"],            "settings": {"foreground": "#FF00FFFF", "fontStyle": ""}},
		{"scope": ["constant.character.v4p.vhdl"],                  "settings": {"foreground": "#800000FF", "fontStyle": ""}},
		{"scope": ["constant.other.generics.v4p.vhdl"],             "settings": {"foreground": "#0000FFFF", "fontStyle": ""}},
		{"scope": ["constant.other.v4p.vhdl"],                      "settings": {"foreground": "#0000FFFF", "fontStyle": ""}},
		{"scope": ["variable.other.signal.v4p.vhdl"],               "settings": {"foreground": "#8000FFFF", "fontStyle": ""}},
		{"scope": ["variable.other.v4p.vhdl"],                      "settings": {"foreground": "#400080FF", "fontStyle": ""}},
		{"scope": ["variable.parameter.v4p.vhdl"],                  "settings": {"foreground": "#1580BFFF", "fontStyle": ""}},
		{"scope": ["variable.parameter.in.v4p.vhdl"],               "settings": {"foreground": "#1580BFFF", "fontStyle": ""}},
		{"scope": ["variable.parameter.out.v4p.vhdl"],              "settings": {"foreground": "#1580BFFF", "fontStyle": ""}},
		{"scope": ["variable.parameter.inoutv4p.vhdl"],             "settings": {"foreground": "#1580BFFF", "fontStyle": ""}},
		{"scope": ["entity.name.interface.v4p.vhdl"],               "settings": {"foreground": "#816641FF", "fontStyle": ""}},
		{"scope": ["meta.namespace.v4p.vhdl"],                      "settings": {"foreground": "#816641FF", "fontStyle": ""}}
	]
}