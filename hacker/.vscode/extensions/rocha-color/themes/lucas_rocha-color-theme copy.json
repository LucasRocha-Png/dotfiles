        }
    },
    {
        "scope": "markup.heading",
        "settings": {
            "foreground": "#1a8fff"
        }
    },
    {
        "scope": "markup.italic",
        "settings": {
            "fontStyle": "italic"
        }
    },
    {
        "scope": "markup.inserted",
        "settings": {
            "foreground": "#19cb00"
        }
    },
    {
        "scope": "markup.deleted",
        "settings": {
            "foreground": "#cc0403"
        }
    },
    {
        "scope": "markup.changed",
        "settings": {
            "foreground": "#1a8fff"
        }
    },
    {
        "scope": [
            "punctuation.definition.tag"
        ],
        "settings": {
            "foreground": "#767676"
        }
    },
    {
        "scope": "meta.preprocessor",
        "settings": {
