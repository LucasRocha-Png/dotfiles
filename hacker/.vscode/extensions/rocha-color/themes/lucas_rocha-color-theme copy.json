        }
    },
    {
        "scope": "storage",
        "settings": {
            "foreground": "#1a8fff"
        }
    },
    {
        "scope": "storage.type",
        "settings": {
            "foreground": "#1a8fff"
        }
    },
    {
        "scope": "storage.modifier",
        "settings": {
            "foreground": "#1a8fff"
        }
    },
    {
        "scope": "string",
        "settings": {
            "foreground": "#cecb00"
        }
    },
    {
        "scope": "string.tag",
        "settings": {
            "foreground": "#cecb00"
        }
    },
    {
        "scope": "string.value",
        "settings": {
