{
  "name": "lucas_rocha-color-theme",
  "type": "dark",
    
  "colors": {
    // CORES GERAIS
    "contrastActiveBorder": "#19cb00", // Borda ativa verde (igual ao i3)
    "contrastBorder": "#000000",
    "focusBorder": "#19cb00",
    "foreground": "#19cb00",
    "widget.shadow": "#000000",
    "selection.background": "#fffacd3f",
    "descriptionForeground": "#0dcdcd", // Descrições em Ciano (igual neofetch)
    "errorForeground": "#cc0403",
    "textLink.foreground": "#0dcdcd", // Links em Ciano

    // EDITOR
    "editor.background": "#000000",
    "editor.foreground": "#dddddd",
    "editor.selectionBackground": "#fffacd",
    "editor.lineHighlightBackground": "#fffacd1f", // Leve brilho azul na linha atual
    "editor.lineHighlightBorder": "#00000000",
    "editorCursor.foreground": "#dddddd", // Cursor branco
    "editorWhitespace.foreground": "#fffacd1f",
    "editorLineNumber.foreground": "#dddddd",
    "editorLineNumber.activeForeground": "#0d73cc", // Linha ativa verde
    "editorRuler.foreground": "#fffacd1f",

    // Popup ao passar o mouse (Hover)
    "editorHoverWidget.background": "#000000",      // Fundo preto
    "editorHoverWidget.border": "#19cb00",          // Borda Verde (para destacar do fundo)
    "editorHoverWidget.foreground": "#19cb00",      // Texto geral
    "editorHoverWidget.highlightForeground": "#cecb00", // Cor de destaque (ex: links dentro do popup)
    "editorHoverWidget.statusBarBackground": "#000000", // Barra inferior do popup (onde diz "Click to follow")

    "scrollbarSlider.background": "#fffacd33",       // Cor normal (transparente)
    "scrollbarSlider.hoverBackground": "#fffacd66",  // Cor quando passa o mouse
    "scrollbarSlider.activeBackground": "#fffacd99", // Cor quando clica para arrastar

    // ABAS (TABS) - Estilo Terminal
    "editorGroupHeader.tabsBackground": "#000000",
    "tab.activeBackground": "#19cb00", // Fundo verde na aba ativa
    "tab.activeForeground": "#000000", // Texto preto na aba ativa (alto contraste)
    "tab.inactiveBackground": "#000000",
    "tab.inactiveForeground": "#19cb00", // Abas inativas em Ciano
    "tab.border": "#000000",
    
    // BARRA LATERAL (SIDEBAR)
    "sideBar.background": "#000000",
    "sideBar.foreground": "#dddddd",
    "sideBar.border": "#19cb00", // Divisória verde fina
    "sideBarTitle.foreground": "#19cb00", // Título em verde (igual 'OS:' do neofetch)
    "sideBarSectionHeader.background": "#000000",
    "sideBarSectionHeader.foreground": "#19cb00",
    
    // ACTIVITY BAR (Ícones na esquerda)
    "activityBar.background": "#000000",
    "activityBar.foreground": "#19cb00", // Ícone ativo verde
    "activityBar.inactiveForeground": "#767676", // Inativo cinza escuro
    "activityBar.border": "#000000",
    "activityBarBadge.background": "#0dcdcd", // Notificações em Ciano
    "activityBarBadge.foreground": "#000000",

    // BARRA DE STATUS (Embaixo)
    "statusBar.background": "#000000",
    "statusBar.foreground": "#19cb00",
    "statusBar.border": "#000000", // Borda superior verde para separar
    "statusBarItem.hoverBackground": "#19cb0022",
    "statusBarItem.remoteBackground": "#0dcdcd",
    "statusBarItem.remoteForeground": "#000000",

    // INPUTS E LISTAS
    "input.background": "#000000",
    "input.foreground": "#19cb00",
    "input.border": "#19cb00",
    "inputOption.activeBackground": "#19cb00",
    "inputOption.activeForeground": "#000000",
    "inputOption.hoverBackground": "#19cb0022",


    "list.activeSelectionBackground": "#19cb00",
    "list.activeSelectionForeground": "#000000",
    "list.hoverBackground": "#19cb0022",

    // TERMINAL INTEGRADO 
    "terminal.background": "#000000",
    "terminal.foreground": "#dddddd",

    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#cc0403",
    "terminal.ansiGreen": "#19cb00", 
    "terminal.ansiYellow": "#cecb00",
    "terminal.ansiBlue": "#0d73cc",
    "terminal.ansiMagenta": "#cb1ed1",
    "terminal.ansiCyan": "#0dcdcd", 
    "terminal.ansiWhite": "#dddddd",

    "terminal.ansiBrightBlack": "#767676",
    "terminal.ansiBrightRed": "#f2201f",
    "terminal.ansiBrightGreen": "#23fd00",
    "terminal.ansiBrightYellow": "#fffd00",
    "terminal.ansiBrightBlue": "#1a8fff",
    "terminal.ansiBrightMagenta": "#fd20ff",
    "terminal.ansiBrightCyan": "#14ffff",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "tokenColors": [
    {
        "scope": [
            "meta.embedded",
            "source.groovy.embedded"
        ],
        "settings": {
            "foreground": "#dddddd", 
            "background": "#000000"
        }
    },
    {
        "scope": "emphasis",
        "settings": {
            "fontStyle": "italic"
        }
    },
    {
        "scope": "strong",
        "settings": {
            "fontStyle": "bold"
        }
    },
    {
        "scope": "meta.diff.header",
        "settings": {
            "foreground": "#0d73cc" 
        }
    },
    {
        "scope": "comment",
        "settings": {
            "foreground": "#767676"
        }
    },
    {
        "scope": "constant.language",
        "settings": {
            "foreground": "#1a8fff"
        }
    },
    { 
        "scope": [
            "constant.numeric",
            "constant.other.color.rgb-value",
            "constant.other.rgb-value",
            "support.constant.color"
        ],
        "settings": {
            "foreground": "#19cb00"
        }
    },
    {
        "scope": "constant.regexp",
        "settings": {
            "foreground": "#cb1ed1"
        }
    },
    {
        "scope": "constant.character",
        "settings": {
            "foreground": "#1a8fff"
        }
    },
    {
        "scope": "entity.name.tag",
        "settings": {
            "foreground": "#1a8fff"
        }
    },
    {
        "scope": "entity.name.tag.css",
        "settings": {
            "foreground": "#cecb00"
        }
    },
    {
        "scope": "entity.other.attribute-name",
        "settings": {
            "foreground": "#14ffff"
        }
    },
    {
        "scope": [
            "entity.other.attribute-name.class.css",
            "entity.other.attribute-name.class.mixin.css",
            "entity.other.attribute-name.id.css",
            "entity.other.attribute-name.parent-selector.css",
            "entity.other.attribute-name.pseudo-class.css",
            "entity.other.attribute-name.pseudo-element.css",
            "source.css.less entity.other.attribute-name.id",
            "entity.other.attribute-name.attribute.scss",
            "entity.other.attribute-name.scss"
